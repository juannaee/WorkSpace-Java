import java.util.Scanner;

public class sequencial_1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Olá Mundo");
		System.out.println("----------------");
		
		String x;
		x = sc.next();
		System.out.println("Você digitou");
		
		
		
		sc.close();

	}

}
